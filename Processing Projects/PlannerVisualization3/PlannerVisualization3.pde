void setup() {
  size(800, 800);
}

void draw() {
  for (int i = ( (month*cols)-cols); i < cols*month; i++) {
    for (int j = 0; j < rows; j++) {
      if (DistrictDensity[i][j] == 0) {
        fill(0);
      } else if (DistrictDensity[i][j] == 1) {
        fill(color2);
      } else if (DistrictDensity[i][j] < 5) {
        fill(color3);
      } else {
        fill(color4);
      }
      rect(((i*width/cols))-width*(month-1), ((j*height/rows)), width/cols, width/cols);
    }
  }
} 


void keyPressed() {
  if (keyCode == DOWN) {
    if (month <= 11) {
      month++;
    } else if (month ==12) {
      month = 1;
    }
  } else if (keyCode == UP) {
    if (month >= 2) {
      month --;
    } else if (month == 1) {
      month = 12;
    }
  }
}

